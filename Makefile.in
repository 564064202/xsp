SUBDIRS = server test doc

.PHONY: all install clean uninstall dist

all:
	for i in $(SUBDIRS); do	\
		$(MAKE) -C $$i || exit 1; \
	done;

install: all
	for i in $(SUBDIRS); do	\
		$(MAKE) -C $$i install || exit 1; \
	done;

clean uninstall:
	for i in $(SUBDIRS); do	\
		$(MAKE) -C $$i $@ || exit 1; \
	done;

